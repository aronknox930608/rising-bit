      "id": "activate-ssh-key_flavor_bitrise",
      "steplib_source": "https://github.com/bitrise-io/bitrise-step-collection",
      "version_tag": "2.0.0",
      "name": "Activate App SSH key",
      "description": "Saves the provided **RSA private SSH key** to file\nand then loads it into the user's ssh-agent with `ssh-add`.\n\n**The key should not have a passphrase** (should have an empty passphrase).\n\n*Warning: the provided key will overwrite an existing key at\nthe provided path (if another key at the path already exists).*\n\nIt will start the ssh-agent if it can't connect to the agent.\n",
      "website": "https://github.com/bitrise-io/steps-activate-ssh-key",
      "fork_url": "https://github.com/bitrise-io/steps-activate-ssh-key",
      "source": {
        "git": "https://github.com/bitrise-io/steps-activate-ssh-key.git"
      },
      "host_os_tags": [
        "osx-10.9",
        "osx-10.10"
      ],
      "project_type_tags": [

      ],
      "type_tags": [
        "ssh"
      ],
      "is_requires_admin_user": false,
      "is_always_run": false,
      "inputs": [
        {
          "title": "SSH private key in RSA format",
          "description": "",
          "mapped_to": "SSH_RSA_PRIVATE_KEY",
          "is_expand": true,
          "is_required": true,
          "value_options": [

          ],
          "value": "$SSH_RSA_PRIVATE_KEY",
          "is_dont_change_value": true
        },
        {
          "title": "Path to save the private key",
          "description": "",
          "mapped_to": "SSH_KEY_SAVE_PATH",
          "is_expand": true,
          "is_required": false,
          "value_options": [

          ],
          "value": "$HOME/.ssh/steplib_ssh_step_id_rsa",
          "is_dont_change_value": true
        },
        {
          "title": "Remove other, previously loaded keys and restart ssh-agent?",
          "description": "",
          "mapped_to": "IS_REMOVE_OTHER_IDENTITIES",
          "is_expand": false,
          "is_required": false,
          "value_options": [
            "true",
            "false"
          ],
          "value": "true",
          "is_dont_change_value": true
        }
      ],
      "outputs": [

      ],
      "log_highlights": [

      ],
      "icon_url_256": "https://bitrise-stepcollection-assets.s3.amazonaws.com/activate-ssh-key_flavor_bitrise/assets/icon_256.png",
      "position_in_workflow": 0
